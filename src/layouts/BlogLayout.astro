---
import BlogHero from "../components/BlogHero.astro";
import Head from "../components/Head.astro";
const { meta } = Astro.props;
---

<!doctype html>
<html lang="en">
  <Head {...meta} />
  <body>
    <main>
      <BlogHero title={meta.title} hero={meta.hero} />
      <slot />
    </main>
  </body>
</html>
